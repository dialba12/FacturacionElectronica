@{ ViewData["Title"] = "NoExisteInventario"; }

<h1 class="heading" style="        text-align: center;
        margin: -19px 0 0;
        margin-top: 20px;
        color: #B8240A;

        font-family: Montserrat;
        font-style: normal;
        font-size: 45px;
        line-height: 78px;
    ">
    No existen unidades de este producto en el inventario
</h1>

<div>
    <form action="ListarInventario" style="left: auto;">



        <input class="rounded border border-danger" value="@TempData["idFactura"]" id="idFactura" name="idFactura" required size="40" type="hidden">

        <input type="submit" value="Regresar" class="btn btn-primary btn-xs"
               style="font-family: Montserrat;
        font-style: normal;
        font-weight: 400;
        font-size: 25px;
        color: #D6F5FA;
        line-height: 44px;
        background: #B8240A;
        border: 2px solid #000000;
        box-shadow: 0px 4px 4px rgba(0, 0, 0, 0.25);
        margin: 10px;">


    </form>


</div>
