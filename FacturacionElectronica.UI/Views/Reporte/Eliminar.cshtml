@model FacturacionElectronica.Modelos.Cierre

@{
    ViewData["Title"] = "EliminarReporte";
}


<h3 class="heading" style="        text-align: center;
        margin: -19px 0 0;
        margin-top: 20px;
        color: #B8240A;

        font-family: Montserrat;
        font-style: normal;
        font-size: 45px;
        line-height: 78px;
    ">
    Estás seguro de eliminar el  reporte @Html.DisplayFor(model => model.idCierre) ?
</h3>
</dl>
<div>


    <form asp-action="Eliminar" style="text-align:center; align-content:center; align-items:center;">
        <input type="submit" value="Si" class="btn btn-danger" />
        <a asp-action="Listar" class="btn btn-success">No</a>
    </form>
</div>
